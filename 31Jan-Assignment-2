                /*Renuka.D ADCGWK  Assignment-2*/
                 /*To print non-zero elements*/
#include <stdio.h>
int main()
{
    int a[]={1,5,0,3,0,7,9,0};
    int i,count=0;
    int n=8;
    for(i=0;i<n;i++) 
    {
        if(a[i]>0)
        {
            a[count++]=a[i];
        }
    }
    while(count<n)
    {
        a[count++]=0;
    }
    for(i=0;i<n;i++) 
    {
        printf("  %d  ",a[i]);
    }
    return 0;
}
